<!-- views/index.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lobby Page</title>
</head>
<body>
    <h1>Choose Code Block</h1>
    <div>
        <ul>
            <% for(let i=0; i<itemList.length; i++) { %>
                <li>
                    <a href="/code_block/<%= itemList[i]._id %>">
                        the id : <%= itemList[i]._id %>
                        The title is: <%= itemList[i].title %><br>
                        The body is: <%= itemList[i].body %>
                    </a>
                </li>
            <% } %>
        </ul>
    </div>
</body>
</html>
